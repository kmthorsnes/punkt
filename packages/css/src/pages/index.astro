---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Punkt CSS - forside">
  <div id="app" class="page-main pkt-container pkt-txt-center">
    <h1>CSS</h1>
    <p>
      <img
        class="pkt-logo"
        alt="Punkt logo"
        aria-hidden="true"
        src="https://punkt-cdn.oslo.kommune.no/latest/icons/punkt-circle-blue.svg"
      />

      <img
        class="pkt-logo"
        alt="Sass logo"
        aria-hidden="true"
        src="https://punkt-cdn.oslo.kommune.no/latest/icons/sass.svg"
      />
    </p>
  </div>
</Layout>
