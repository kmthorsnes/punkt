---
import '../main.scss'

export interface Props {
  title: string
}

const { title } = Astro.props
---

<!DOCTYPE html>
<html lang="no">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
    <meta name="robots" content="noindex,nofollow" />
    <title>{title}</title>
  </head>
  <body>
    <div id="page-container" class="page-container">
      <header id="dev-header" class="dev-header" aria-label="Topp">
        <div class="dev-header__logo">
          <a
            aria-label="Tilbake til forside"
            class="dev-header__logo-link"
            href="/"
          >
            <span class="pkt-icon dev-header__logo-svg">
              <svg viewBox="0 0 60 31">
                <use href="#oslologo"></use>
              </svg>
            </span>
          </a>
        </div>

        <div class="toolbar">
          <div class="toolbar-darkmode">
            <div class="pkt-input-check__input">
              <label
                class="pkt-input-check__input-label page-main__label"
                for="toolbar-mode-toggle"
              >
                Dark mode
              </label>
              <input
                type="checkbox"
                role="switch"
                class="pkt-input-check__input-checkbox"
                id="toolbar-mode-toggle"
              />
            </div>
          </div>
          <script is:inline>
            const cb = document.getElementById('toolbar-mode-toggle')
            const bodyEl = document.querySelector('body')

            function setMode() {
              cb.checked
                ? bodyEl.setAttribute('data-mode', 'dark')
                : bodyEl.setAttribute('data-mode', 'light')
            }

            cb.addEventListener('click', () => {
              setMode()
            })

            setMode()
          </script>
        </div>

        <nav class="dev-header__nav">
          <select
            class="pkt-input"
            name="dev-header__select"
            id="dev-header__select"
          >
            <option value="/" id="home" selected>Choose component</option>
            <option value="/alert" id="alert">Alert</option>
            <option value="/backlink" id="backlink">Backlink</option>
            <option value="/breadcrumbs" id="breadcrumbs">Breadcrumbs</option>
            <option value="/button" id="button">Button</option>
            <option value="/checkboxradio" id="checkboxradio"
              >Checkbox and Radio</option
            >
            <option value="/footer" id="footer">Footer</option>
            <option value="/grid" id="grid">Grid</option>
            <option value="/linkcard" id="linkcard">Links and LinkCard</option>
            <option value="/header" id="header">Header</option>
            <option value="/messagebox" id="messagebox">Messagebox</option>
            <option value="/searchinput" id="select">Search input</option>
            <option value="/select" id="select">Select</option>
            <option value="/switch" id="switch">Switch</option>
            <option value="/tag" id="tag">Tag</option>
            <option value="/template-404" id="template-404">Template 404</option
            >
            <option value="/tabs" id="tabs">Tabs</option>
            <option value="/textarea" id="textarea">Text area</option>
            <option value="/textinput" id="textinput">Text input</option>
            <option value="/typografi" id="typografi">Typografi</option>
          </select>
          <script is:inline>
            const selectBox = document.getElementById('dev-header__select')
            const component = window.location.pathname.split('/')[1]

            selectBox.addEventListener('change', (event) => {
              window.location.href =
                event.target.options[event.target.selectedIndex].value
            })

            if (component) {
              selectBox.value = `/${component}`
            }
          </script>
        </nav>
      </header>

      <slot />
    </div>

