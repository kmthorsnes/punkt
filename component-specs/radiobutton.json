{
  "name": "pkt-radiobutton",
  "css-class": "pkt-input-check",
  "props": {
    "id": {
      "type": "String",
      "required": true
    },
    "name": {
      "type": "String",
      "required": true
    },
    "label": {
      "type": "String",
      "required": true
    },
    "selectedValue": {
      "type": "String",
      "required": false,
      "default": ""
    },
    "hasTile": {
      "type": "Boolean",
      "required": false,
      "default": false
    },
    "disabled": {
      "type": "Boolean",
      "required": false,
      "default": false
    },
    "checkHelptext": {
      "type": "String",
      "required": false
    },
    "value": {
      "type": "String",
      "required": false
    },
    "hasError": {
      "type": "Boolean",
      "required": false,
      "default": false
    }
  }
}
